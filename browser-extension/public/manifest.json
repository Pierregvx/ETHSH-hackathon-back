{
  "name": "Build Verifier",
  "description": "DAO Extension Demo for Eth SF",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "ethglobalLogo.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>",
    "activeTab",
    "tabs"],
  "web_accessible_resources": ["popup.html", "background.js"]
}